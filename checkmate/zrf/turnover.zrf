(define pawn-step (
  $1 add
))

(define leap (
  $1 $2 add
))

(define check-empty
  mark
  (if (on-board? $1) 
      $1 (verify empty?)
  )
  (if (on-board? $1) 
      $1 (verify empty?)
  )
  back
)

(define pawn-jump (
  $1 (verify empty?)
  (check-empty up)
  (check-empty dn)
  $1 add
))

(define slide (
  $1 
  (while empty?
     add
     (check-empty up)
     (check-empty dn)
     $1
  )
  add
))

